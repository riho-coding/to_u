{
	"root": true,
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"rules": {
		"no-unused-vars": "warn",
		"semi": [
			"warn",
			"always"
		],
		"semi-spacing": [
			"error",
			{
				"after": true,
				"before": false
			}
		],
		"semi-style": [
			"error",
			"last"
		],
		"no-extra-semi": "warn",
		"no-unexpected-multiline": "error",
		"no-unreachable": "error",
		"no-undef": 0
	},
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020
	},
	"globals": {
		"process": true
	}
}
